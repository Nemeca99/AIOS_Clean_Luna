[run]
# Omit vendor code and stub files from coverage
omit = 
    streamlit_core/*
    streamlit_core/**/*
    */__init__.py
    .venv/*
    venv/*
    */site-packages/*
    archive_dev_core/*
    */tests/*
    */test_*.py
    setup.py
    *_version.py

[report]
# Reporting options
precision = 2
show_missing = True
skip_covered = False

# Fail if coverage is below threshold (90%)
fail_under = 90

# Exclude patterns
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    pass

