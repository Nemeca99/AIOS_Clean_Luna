{
  "version": "2.0.0",
  "schema_version": "1.0",
  "name": "RAG Core - Manual Oracle",
  "description": "Manual Oracle system with GPU-accelerated embeddings",
  "embedder": {
    "model": "all-MiniLM-L6-v2",
    "dimensions": 384,
    "device": "auto"
  },
  "oracle": {
    "index_file": "manual_oracle/oracle_index.json",
    "manual_file": "../AIOS_MANUAL.md",
    "toc_file": "../MANUAL_TOC.md",
    "use_mmap": true
  },
  "search": {
    "default_top_k": 5,
    "min_score": 0.5,
    "two_pass": true
  }
}

